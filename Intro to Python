a_list = [20, 22, 18, 23, 11, 14, 17, 12, 32, 13]
mul_list = []

#nested loop to create rows and columns
for i in range(2):
    row = []
    for j in range(5):
        row.append(a_list[i*5 + j])
    mul_list.append(row)
table = [a_list[:5], a_list[5:]]

print(mul_list)
